<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
       xmlns:c="http://www.springframework.org/schema/c"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--p命名注入，可以直接注入属性的值，property-->
    <bean id="user" class="com.yuan.pojo.User" p:name="567" p:age="18" />

    <!--c命名注入,通过构造器注入，construct-args  (scope="prototype"原型模式)默认是单例模式-->
    <bean id="user2" class="com.yuan.pojo.User" c:name="789" c:age="79" scope="prototype"/>

</beans>